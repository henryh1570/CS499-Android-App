<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hu.henry.sleepkeep">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MenuActivity"
            android:label="MenuActivity"
            android:parentActivityName=".MainActivity"></activity>

        <activity
            android:name=".DeleteDataActivity"
            android:label="DeleteDataActivity"
            android:parentActivityName=".MenuActivity"></activity>

        <activity
            android:name=".DebugActivity"
            android:label="DebugActivity"
            android:parentActivityName=".MenuActivity"></activity>

        <activity
            android:name=".ProfileActivity"
            android:label="ProfileActivity"
            android:parentActivityName=".MenuActivity"></activity>

        <activity
            android:name=".SleepActivity"
            android:label="SleepActivity"
            android:parentActivityName=".MenuActivity"></activity>

        <activity
            android:name=".SleepTodayActivity"
            android:label="SleepTodayActivity"
            android:parentActivityName=".SleepActivity"></activity>

        <activity
            android:name=".SleepPreviousActivity"
            android:label="SleepPreviousActivity"
            android:parentActivityName=".SleepActivity"></activity>

        <activity
            android:name=".StatsActivity"
            android:label="StatsActivity"
            android:parentActivityName=".MenuActivity"></activity>

    </application>

</manifest>